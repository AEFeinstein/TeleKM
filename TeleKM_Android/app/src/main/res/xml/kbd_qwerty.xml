<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="0px">

    <Row android:rowEdgeFlags="top">
        <Key
            android:codes="@integer/KEYCODE_1"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="@integer/KEYCODE_2"
            android:keyLabel="2" />
        <Key
            android:codes="@integer/KEYCODE_3"
            android:keyLabel="3" />
        <Key
            android:codes="@integer/KEYCODE_4"
            android:keyLabel="4" />
        <Key
            android:codes="@integer/KEYCODE_5"
            android:keyLabel="5" />
        <Key
            android:codes="@integer/KEYCODE_6"
            android:keyLabel="6" />
        <Key
            android:codes="@integer/KEYCODE_7"
            android:keyLabel="7" />
        <Key
            android:codes="@integer/KEYCODE_8"
            android:keyLabel="8" />
        <Key
            android:codes="@integer/KEYCODE_9"
            android:keyLabel="9" />
        <Key
            android:codes="@integer/KEYCODE_0"
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/KEYCODE_q"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />
        <Key
            android:codes="@integer/KEYCODE_w"
            android:keyLabel="w" />
        <Key
            android:codes="@integer/KEYCODE_e"
            android:keyLabel="e" />
        <Key
            android:codes="@integer/KEYCODE_r"
            android:keyLabel="r" />
        <Key
            android:codes="@integer/KEYCODE_t"
            android:keyLabel="t" />
        <Key
            android:codes="@integer/KEYCODE_y"
            android:keyLabel="y" />
        <Key
            android:codes="@integer/KEYCODE_u"
            android:keyLabel="u" />
        <Key
            android:codes="@integer/KEYCODE_i"
            android:keyLabel="i" />
        <Key
            android:codes="@integer/KEYCODE_o"
            android:keyLabel="o" />
        <Key
            android:codes="@integer/KEYCODE_p"
            android:keyEdgeFlags="right"
            android:keyLabel="p" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/KEYCODE_a"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="a" />
        <Key
            android:codes="@integer/KEYCODE_s"
            android:keyLabel="s" />
        <Key
            android:codes="@integer/KEYCODE_d"
            android:keyLabel="d" />
        <Key
            android:codes="@integer/KEYCODE_f"
            android:keyLabel="f" />
        <Key
            android:codes="@integer/KEYCODE_g"
            android:keyLabel="g" />
        <Key
            android:codes="@integer/KEYCODE_h"
            android:keyLabel="h" />
        <Key
            android:codes="@integer/KEYCODE_j"
            android:keyLabel="j" />
        <Key
            android:codes="@integer/KEYCODE_k"
            android:keyLabel="k" />
        <Key
            android:codes="@integer/KEYCODE_l"
            android:keyEdgeFlags="right"
            android:keyLabel="l" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/KEYCODE_SHIFT"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift_lxx_dark" />

        <Key
            android:codes="@integer/KEYCODE_z"
            android:keyLabel="z" />
        <Key
            android:codes="@integer/KEYCODE_x"
            android:keyLabel="x" />
        <Key
            android:codes="@integer/KEYCODE_c"
            android:keyLabel="c" />
        <Key
            android:codes="@integer/KEYCODE_v"
            android:keyLabel="v" />
        <Key
            android:codes="@integer/KEYCODE_b"
            android:keyLabel="b" />
        <Key
            android:codes="@integer/KEYCODE_n"
            android:keyLabel="n" />
        <Key
            android:codes="@integer/KEYCODE_m"
            android:keyLabel="m" />
        <Key
            android:codes="@integer/KEYCODE_HORIZONTAL_TAB"
            android:keyIcon="@drawable/sym_keyboard_tab_lxx_dark" />
        <Key
            android:codes="@integer/KEYCODE_BACKSPACE"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete_lxx_dark" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/KEYCODE_MODE_CHANGE"
            android:keyEdgeFlags="left"
            android:keyLabel="!@#$"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYCODE_CTRL"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="CTRL"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYCODE_SUPER"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="SUPR"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYCODE_SPACE"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space_lxx_dark"
            android:keyWidth="25%p" />
        <Key
            android:codes="@integer/KEYCODE_ALT"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="ALT"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYCODE_ENTER"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return_lxx_dark"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>